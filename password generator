import random
lower_case = "abcdefghijklmnopqrstuvwxyz"
upper_case = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
numbers = "0123456789"
special_chars = "~}{][)(*&?|=<>\/!$%.-_:;"
all = lower_case + upper_case + numbers + special_chars
length = 20
password = "".join(random.sample(all, length))
		   print (password)
